{
    "$schema": "https://schema.management.azure.com/schemas/2018-05-01/subscriptionDeploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "rgName": {
            "type": "string",
            "metadata": {
                "description": "Name of the resourceGroup to create"
            }
        },
        "rgLocation": {
            "type": "string",
            "metadata": {
                "description": "Location for the resourceGroup"
            }
        },
        "powerStigVersion": {
            "type": "string",
            "metadata": {
                "description": "Version of PowerSTIG to use. Previous default was 4.7.1"
            },
            "defaultValue": "4.13.1"
        },
        "storageAccountResourceGroup": {
            "type": "string",
            "metadata": {
                "description": "Resource group of the installation scripts storage account"
            }
        },
        "storageAccountName": {
            "type": "string",
            "metadata": {
                "description": "Storage account to pull installation script from"
            }
        },
        "storageContainerName": {
            "type": "string",
            "metadata": {
                "description": "Storage container to pull installation script from"
            }
        },
        "customScriptFileName": {
            "type": "string",
            "metadata": {
                "description": "File name of the custom script to run, including the extension"
            }
        },
        "customBinaryPackageName": {
            "type": "string",
            "metadata": {
                "description": "File name of a package that the custom script uses i.e. zip file containing installation media"
            }
        },
        "sasToken": {
            "type": "string",
            "metadata": {
                "description": "SASToken used to retrieve blobs from storage account"
            }
        }
    },
    "functions": [],
    "variables": {
        "workspaceUrl": "[uri(deployment().properties.templateLink.uri, 'nested/workspace.json')]",
        "workspaceDashboardUrl": "[uri(deployment().properties.templateLink.uri, 'nested/dashboard.json')]",
        "managedIdentityUrl": "[uri(deployment().properties.templateLink.uri, 'nested/managedIdentity.json')]",
        "managedIdentityAccessUrl": "[uri(deployment().properties.templateLink.uri, 'nested/managedIdentityAccess.json')]",
        "imageGalleryUrl": "[uri(deployment().properties.templateLink.uri, 'nested/imageGallery.json')]",
        "srv2019imageTemplateUrl": "[uri(deployment().properties.templateLink.uri, 'imageTemplates/windows2019.json')]",
        "custom2019imageTemplateUrl": "[uri(deployment().properties.templateLink.uri, 'imageTemplates/windows2019_custom.json')]",
        "imageDefinitionUrl": "[uri(deployment().properties.templateLink.uri, 'nested/imageDefinition.json')]",
        "srv2019MofUrl": "[uri(deployment().properties.templateLink.uri, 'mof/WindowsServer2019v2r4/localhost.mof')]",
        "customScriptUri": "[concat('https://', parameters('storageAccountName'), '.blob.core.windows.net/', parameters('storageContainerName'), '/', parameters('customScriptFileName'), parameters('sasToken'))]",
        "customBinaryPackageUri": "[concat('https://', parameters('storageAccountName'), '.blob.core.windows.net/', parameters('storageContainerName'), '/', parameters('customBinaryPackageName'), parameters('sasToken'))]",
    },
    "resources": [
        {
            "type": "Microsoft.Resources/resourceGroups",
            "apiVersion": "2019-10-01",
            "name": "[parameters('rgName')]",
            "location": "[parameters('rgLocation')]",
            "tags": {
                "Note": "subscription level deployment"
            },
            "properties": {}
        },
        {
            "name": "workspaceDeploy",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2019-10-01",
            "resourceGroup": "[parameters('rgName')]",
            "dependsOn": [
                "[resourceId('Microsoft.Resources/resourceGroups/', parameters('rgName'))]"
            ],
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[variables('workspaceUrl')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "name": {
                        "value": "[parameters('rgName')]"
                    }
                }
            }
        },
        {
            "name": "managedIdentityDeploy",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2019-10-01",
            "resourceGroup": "[parameters('rgName')]",
            "dependsOn": [
                "[resourceId('Microsoft.Resources/resourceGroups/', parameters('rgName'))]"
            ],
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[variables('managedIdentityUrl')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "name": {
                        "value": "[parameters('rgName')]"
                    }
                }
            }
        },
        // {
        //     "name": "managedIdentityStorageAccessDeploy",
        //     "type": "Microsoft.Resources/deployments",
        //     "apiVersion": "2019-10-01",
        //     "resourceGroup": "[parameters('rgName')]",
        //     "dependsOn": [
        //         "managedIdentityDeploy"
        //     ],
        //     "properties": {
        //         "mode": "Incremental",
        //         "templateLink": {
