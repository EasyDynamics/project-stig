        //             "contentVersion": "1.0.0.0"
        //         },
        //         "parameters": {
        //             "storageAccountResourceGroup": {
        //                 "value": "[parameters('storageAccountResourceGroup')]"
        //             },
        //             "storageAccountName": {
        //                 "value": "[parameters('storageAccountName')]"
        //             },
        //             "identityPrincipalId": {
        //                 "value": "[reference('managedIdentityDeploy').outputs.identityPrincipalId.value]"
        //             }
        //         }
        //     }
        // },
        {
            "name": "imageGalleryDeploy",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2019-10-01",
            "resourceGroup": "[parameters('rgName')]",
            "dependsOn": [
                "managedIdentityDeploy"
            ],
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[variables('imageGalleryUrl')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "name": {
                        "value": "[parameters('rgName')]"
                    }
                }
            }
        },
        {
            "name": "imageDefinitionsDeploy",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2019-10-01",
            "resourceGroup": "[parameters('rgName')]",
            "dependsOn": [
                "imageGalleryDeploy"
            ],
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[variables('imageDefinitionUrl')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "galleryName": {
                        "value": "[reference('imageGalleryDeploy').outputs.galleryName.value]"
                    }
                }
            }
        },
        // {
        //     "name": "windows2019STIGtemplate",
        //     "type": "Microsoft.Resources/deployments",
        //     "apiVersion": "2019-10-01",
        //     "resourceGroup": "[parameters('rgName')]",
        //     "dependsOn": [
        //         "imageDefinitionsDeploy"
        //     ],
        //     "properties": {
        //         "mode": "Incremental",
        //         "templateLink": {
        //             "uri": "[variables('srv2019imageTemplateUrl')]",
