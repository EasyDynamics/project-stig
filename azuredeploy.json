        //         },
        //         "parameters": {
        //             "identityId": {
        //                 "value": "[reference('managedIdentityDeploy').outputs.identityName.value]"
        //             },
        //             "mofUrl": {
        //                 "value": "[variables('srv2019MofUrl')]"
        //             },
        //             "galleryName": {
        //                 "value": "[reference('imageGalleryDeploy').outputs.galleryName.value]"
        //             },
        //             "workspaceName": {
        //                 "value": "[reference('workspaceDeploy').outputs.workspaceName.value]"
        //             },
        //             "imageTemplateName": {
        //                 "value": "Win2019_STIG"
        //             },
        //             "powerStigVersion": {
        //                 "value": "[parameters('powerStigVersion')]"
        //             },
        //             "stigVersion": {
        //                 "value": "2.4"
        //             },
        //             "osVersion": {
        //                 "value": "2019"
        //             },
        //             "osRole": {
        //                 "value": "MS"
        //             }
        //         }
        //     }
        // },
        {
            "name": "custom2019imageTemplateUrl",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2019-10-01",
            "resourceGroup": "[parameters('rgName')]",
            "dependsOn": [
                "imageDefinitionsDeploy"
            ],
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[variables('custom2019imageTemplateUrl')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "identityId": {
                        "value": "[reference('managedIdentityDeploy').outputs.identityName.value]"
                    },
                    "mofUrl": {
                        "value": "[variables('srv2019MofUrl')]"
                    },
                    "galleryName": {
                        "value": "[reference('imageGalleryDeploy').outputs.galleryName.value]"
                    },
                    "workspaceName": {
                        "value": "[reference('workspaceDeploy').outputs.workspaceName.value]"
                    },
                    "imageTemplateName": {
                        "value": "Win2019_STIG_Custom"
                    },
                    "powerStigVersion": {
                        "value": "[parameters('powerStigVersion')]"
                    },
                    "stigVersion": {
                        "value": "2.4"
                    },
                    "osVersion": {
                        "value": "2019"
                    },
                    "osRole": {
                        "value": "MS"
                    },
                    "customScriptUri": {
                        "value": "[variables('customScriptUri')]"
                    },
                    "customBinaryPackageUri": {
                        "value": "[variables('customBinaryPackageUri')]"
                    },
                    "customBinaryPackageName": {
                        "value": "[parameters('customBinaryPackageName')]"
                    }
                }
            }
        }
    ],
    "outputs": {}
}